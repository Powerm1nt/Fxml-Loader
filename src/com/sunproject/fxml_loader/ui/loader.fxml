<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sunproject.fxml_loader.Controller">
   <center>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select Fxml File to load ..." BorderPane.alignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Text>
   </center>
   <right>
      <Button fx:id="chooseFile" onAction="#chooseFile" mnemonicParsing="false" text="File..." BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="8.0" left="15.0" right="8.0" top="8.0" />
         </BorderPane.margin>
      </Button>
   </right>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</BorderPane>
